# The build environment for the ui repo.
# Source this in your .bashrc

export VPLEX_ROOT="$HOME/workspace/vplex"
export DOLPHIN_ROOT="$HOME/workspace/vplex"

export UI_DEV_DIR="$VPLEX_ROOT/ui"
export UI_REVIEW_DIR="$VPLEX_ROOT/review/ui"
export UI_TEST_DIR="$VPLEX_ROOT/test/ui"
#export UI_REGRESSION_DIR="$VPLEX_ROOT/test/ui-regression"

export NSFW_DEV_DIR="$VPLEX_ROOT/nsfw"
export NSFW_ROOT=$NSFW_DEV_DIR
export NSFW_REVIEW_DIR="$VPLEX_ROOT/review/nsfw"
export RUNNER_TMP="$VPLEX_ROOT/runner-tmp"

export VATS_HOME="$VPLEX_ROOT/vats"

export DIST_PYTHON='/usr/bin/python2'

export SMS_V2_JAVA_HOME='/usr/lib/jvm/java-8-openjdk'
export SMS_V2_MAVEN_HOME='/opt/maven'
export SMS_V2_ANT_HOME='/usr/share/apache-ant'


#dolphin
export DOLPHIN_DEV_DIR="$DOLPHIN_ROOT"
export GOPATH="$HOME/.go"
export GOBIN="$GOPATH/bin"

#export PATH="$HOME/.bin:$PATH:$RUNNER_PATH:$SPGEAR_TOOLS_EXTRA:$GOBIN"
export PATH="$HOME/.bin:$GOBIN:$PATH"

return 0
